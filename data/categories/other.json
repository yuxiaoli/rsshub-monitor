    "route_key": "/:city/:pollution?",
    "route": {
      "path": "/:city/:pollution?",
      "example": "/aqicn/beijing/pm25",
      "parameters": {
        "city": "城市拼音或地区 ID，详见[aqicn.org](http://aqicn.org/city/)",
