{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "url": {
        "type": "string",
        "format": "uri"
      },
      "location": {
        "type": "string"
      },
      "maintainer": {
        "type": "string"
      },
      "maintainerUrl": {
        "type": "string",
        "format": "uri"
      },
      "status": {
        "type": "boolean"
      }
    },
    "required": ["url", "location", "maintainer", "maintainerUrl", "status"]
  }
}
